msgid "_TOOLTIP_SYSTEM_DB"
msgstr "DataBase is only use to save alerts and to execute SQL Queries on Reports.<br> <b>Server:</b> (<i>default: localhost</i>)<br> <b>User:</b> (<i>default: octopussy</i>)<br> <b>Password:</b> (<i>default: octopussy</i>)<br> <b>Base:</b> (<i>default: octopussy</i>)<br>"

msgid "_TOOLTIP_SYSTEM_JABBER"
msgstr "Jabber is used to send Alerts to Instant Messaging Contacts.<br> <b>Server:</b> (<i>yourjabber.yourdomain.com</i>)<br> <b>User:</b> (<i>yourjabberuser</i>)<br> <b>Password:</b> (<i>yourjabberpassword</i>)<br> <hr> More info on Jabber: http://www.jabber.org/"

msgid "_TOOLTIP_SYSTEM_LDAP"
msgstr "LDAP is used to get Contacts and Users.<br> <b>Contacts Server:</b> (<i>yourldapserver.yourdomain.com</i>)<br> <b>Contacts Base:</b> (<i>ou=Contacts,dc=yourdomain,dc=com</i>)<br> <b>Contacts Filter:</b> <br> <b>Users Server:</b> (<i>yourldapserver.yourdomain.com</i>)<br> <b>Users Base:</b> (<i>ou=Users,dc=yourdomain,dc=com</i>)<br> <b>Users Filter:</b> <br> <hr> More info on LDAP: http://www.openldap.org/"

msgid "_TOOLTIP_SYSTEM_NSCA"
msgstr "NSCA is used to send Alerts to the Nagios supervision console.<br> <b>Binary:</b> Name of the binary send_nsca<br> <b>Configuration File:</b> Filename of your send_nsca configuration file<br> <b>Nagios Server:</b> Address of your Nagios supervision console<br> <b>Host defined in Nagios:</b> Name of the Host defined in Nagios to handle Octopussy Alerts<br> <b>Service defined in Nagios:</b> Name of the Service defined in Nagios to handle Octopussy Alerts<br> <hr> More info on Nagios and NSCA: http://www.nagios.org/"

msgid "_TOOLTIP_SYSTEM_PROXY"
