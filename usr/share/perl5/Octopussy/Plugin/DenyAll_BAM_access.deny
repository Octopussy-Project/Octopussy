# access-20050218.deny
# 09000: Global: "/..":
# - uri: uri?/../ | uri?../
# - arg: param=/../ | param=../
# - data: param=/../ | param=../
# - misc: ".../....///" (=> "../")
# CVE:  / BugTraq:  / Nessus: 
EAccessUriRule deny=407	"^(GET|HEAD|POST) [^?|]*[?|][/\]?\.\.[/\]"
EAccessUriRule deny=407	"[=&][/\]?\.\.[/\]"
EAccessUriRule deny=407	"^(GET|HEAD|POST) [^?|]*\.\.\.[/\]\.\.\.\.[/\][/\][/\]"
